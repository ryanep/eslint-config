{
  "name": "@ryanep/eslint-config",
  "version": "0.5.2",
  "description": "A personal ESLint config",
  "keywords": [
    "eslint",
    "config",
    "ryanep"
  ],
  "license": "MIT",
  "main": "./lib",
  "files": [
    "readme.md",
    "lib"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "node -r ts-node/register ./scripts/clean",
    "dev": "nodemon",
    "format": "prettier --write .",
    "generate": "node -r ts-node/register ./scripts/generate",
    "lint": "eslint './**/*.{ts,json}'",
    "precommit": "lint-staged",
    "release": "semantic-release",
    "scan": "node -r ts-node/register/transpile-only ./scripts/scan"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@next/eslint-plugin-next": "^13.5.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsonc": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.1.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "globals": "^13.23.0",
    "jsonc-eslint-parser": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@ryanep/eslint-config": "^0.3.1",
    "@ryanep/prettier-config": "^1.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/eslint": "^8.44.4",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "eslint": "^8.51.0",
    "eslint-plugin-perfectionist": "^2.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "semantic-release": "^22.0.5",
    "sort-package-json": "^2.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "prettier": "^3.0.3",
    "@typescript-eslint/parser": "^6.8.0"
  }
}

{
  "name": "@ryanep/eslint-config",
  "description": "A personal ESLint config",
  "version": "0.0.18",
  "main": "./lib",
  "license": "MIT",
  "keywords": [
    "eslint",
    "config",
    "rep"
  ],
  "files": [
    "readme.md",
    "base.js",
    "jest.js",
    "json.js",
    "react.js",
    "tailwind.js",
    "typescript.js",
    "next.js",
    "prettier.js",
    "testing-library.js",
    "typescript.js"
  ],
  "scripts": {
    "dev": "nodemon",
    "generate": "node -r ts-node/register ./scripts/generate",
    "clean": "node -r ts-node/register ./scripts/clean",
    "lint": "eslint './**/*.{ts,json}'",
    "build": "tsc --project tsconfig.build.json",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@next/eslint-plugin-next": "^12.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-jsonc": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.6.4",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "jsonc-eslint-parser": "^2.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@ryanep/eslint-config": "^0.0.14",
    "@ryanep/prettier-config": "^1.0.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/eslint": "^8.4.6",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.15",
    "eslint": "^8.23.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "sort-package-json": "^1.57.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}

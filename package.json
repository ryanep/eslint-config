{
  "name": "@ryanep/eslint-config",
  "version": "0.9.1",
  "description": "A personal ESLint config",
  "keywords": [
    "config",
    "eslint",
    "ryanep"
  ],
  "license": "MIT",
  "main": "./lib",
  "files": [
    "lib",
    "readme.md"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "node -r ts-node/register ./scripts/clean",
    "dev": "nodemon",
    "format": "prettier --write .",
    "generate": "node -r ts-node/register ./scripts/generate",
    "lint": "eslint './**/*.{ts,json,graphql,yml,yaml}'",
    "precommit": "lint-staged",
    "release": "semantic-release",
    "scan": "node -r ts-node/register/transpile-only ./scripts/scan"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@next/eslint-plugin-next": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsdoc": "^48.0.6",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.2.0",
    "eslint-plugin-tailwindcss": "^3.14.2",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-yml": "^1.12.2",
    "globals": "^14.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "yaml-eslint-parser": "^1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@ryanep/eslint-config": "^0.9.1",
    "@ryanep/prettier-config": "^1.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/eslint": "^8.56.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "sort-package-json": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "prettier": "^3.2.5",
    "@typescript-eslint/parser": "^6.21.0"
  }
}
